{
  "name": "statecraft",
  "version": "1.1.3",
  "private": true,
  "packageManager": "pnpm@9.14.2",
  "scripts": {
    "build": "pnpm --filter @stcrft/statecraft-core run build && pnpm --filter @stcrft/statecraft-renderer run build && pnpm --filter @stcrft/statecraft run build",
    "dev": "pnpm --filter @stcrft/statecraft-renderer run dev",
    "test": "pnpm -r run test",
    "test:e2e": "pnpm --filter @stcrft/statecraft-renderer run test:e2e",
    "test:coverage": "pnpm -r run test:coverage",
    "cli": "node packages/cli/dist/index.js"
  },
  "devDependencies": {
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/git": "^10.0.0",
    "@semantic-release/github": "^12.0.0",
    "@semantic-release/release-notes-generator": "^14.0.0",
    "semantic-release": "^24.0.0"
  }
}
